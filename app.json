{
  "expo": {
    "name": "Scorausta",
    "slug": "Scorausta",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Scorausta"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "http://localhost:8081/api/kenttapi"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Tarvitsemme pääsyn kameraan videoiden kuvaamista varten.",
          "microphonePermission": "Tarvitsemme pääsyn mikrofoniin videoiden äänen tallentamista varten."
        }
      ],
      "expo-sqlite",
      "expo-asset",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
